  "dependencies": {
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.2",
    "htmlparser2": "^3.10.0",
    "prompts": "^1.2.1",
    "yargs-parser": "^11.1.1"
  },
  "contributors": [{
      "name": "Adam Curl",
      "email": "adam@sodiumhalogen.com",
      "url": "http://github.com/adamcurl"
    },
    {
      "name": "Zach Boatwright",
      "email": "szboatwright@gmail.com",
